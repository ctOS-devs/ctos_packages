pkgname=your_package
pkgver=1.0
pkgrel=0
pkgdesc="Your package description"
url="example.com"
arch="x86_64"
license="NO LICENSE"
depends=""
makedepends=""
install=""
subpackages=""
source=""
builddir=$PWD/$pkgname*

build() {
    return 0
}

package() {
    mkdir -p "$pkgdir"
    ls "$pkgdir/"
    cp -r $builddir/* $pkgdir
    ls "$pkgdir/"
}
