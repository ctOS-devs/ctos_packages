pkgname=make
pkgver=4.4
pkgrel=0
pkgdesc="GNU Make utility"
url="https://www.gnu.org/software/make/"
arch="x86_64"
license="GPL-3.0-or-later"
depends=""
makedepends=""
install=""
subpackages=""
source=""
builddir=$PWD/$pkgname*

build() {
    return 0
}

package() {
    mkdir -p "$pkgdir"
    ls "$pkgdir/"
    cp -r $builddir/* $pkgdir
    ls "$pkgdir/"
}
